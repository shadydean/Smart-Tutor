{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    },
    {
      "src": "^/[^.]+$",
      "dest": "frontend/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://your-backend-url.com"
  }
} 